{
  "name": "es6-fuzz",
  "description": "fuzzy logic with and for es6",
  "version": "2.4.1",
  "main": "index.js",
  "repository": "git@github.com:sebs/es6-fuzz.git",
  "scripts": {
    "env": "env",
    "changelog": "git log `git describe --tags --abbrev=0`..HEAD >> changelog.md",
    "test": "./node_modules/mocha/bin/mocha -r blanket --compilers js:mocha-traceur",
    "test:watch": "npm test -- -w",
    "jscs": "./node_modules/jscs/bin/jscs --esnext lib/logic.js lib/curve/* test/*.js",
    "preversion": "npm run changelog && git add . && git commit -m '(chore): docs'",
    "postversion": "git push && git push --tags"
  },
  "keywords": [
    "fuzzylogic",
    "fuzzy",
    "rule engine",
    "javascript",
    "es6"
  ],
  "config": {
    "blanket": {
      "pattern": "lib/*",
      "data-cover-never": [
        "node_modules",
        "tests"
      ]
    }
  },
  "author": "Sebastian Sch√ºrmann",
  "license": "MIT",
  "devDependencies": {
    "browserify": "^13.0.0",
    "documentation": "^3.0.1",
    "es6ify": "^1.6.0",
    "eslint": "^3.3.0",
    "jscs": "^3.0.4",
    "jsdoc": "^3.3.3",
    "mocha": "^3.1.0",
    "mocha-traceur": "^2.1.0",
    "traceur": "0.0.111"
  },
  "dependencies": {
    "blanket": "^1.2.3"
  }
}
