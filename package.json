{
  "name": "es6-fuzz",
  "description": "fuzzy logic with and for es6",
  "version": "1.7.0",
  "main": "index.js",
  "repository": "git@github.com:sebs/es6-fuzz.git",
  "scripts": {
    "env": "env",
    "changelog": "git log `git describe --tags --abbrev=0`..HEAD >> changelog.md",
    "test": "./node_modules/mocha/bin/mocha --compilers js:mocha-traceur",
    "test:watch": "mpm test -- -w",
    "jscs": "./node_modules/jscs/bin/jscs --esnext lib/logic.js lib/curve/* test/*.js",
    "docs": "./node_modules/documentation/bin/documentation.js lib/logic.js lib/curve/* -f html -o out",
    "docs:site": "./node_modules/documentation/bin/documentation.js lib/logic.js lib/curve/* -f md -o api.md"
  },
  "keywords": [
    "fuzzylogic",
    "fuzzy",
    "rule engine",
    "javascript",
    "es6"
  ],
  "author": "Sebastian Sch√ºrmann",
  "license": "MIT",
  "devDependencies": {
    "browserify": "^11.2.0",
    "documentation": "^2.1.0-alpha2",
    "es6ify": "^1.6.0",
    "eslint": "^1.6.0",
    "jscs": "^2.3.0",
    "jsdoc": "^3.3.3",
    "mocha": "^2.3.3",
    "mocha-traceur": "^2.1.0",
    "traceur": "0.0.92"
  }
}
